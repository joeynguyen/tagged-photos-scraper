{
  "name": "tagged-photos-scraper",
  "version": "1.0.0",
  "description": "Scrape Facebook for photos you're tagged in",
  "author": "Joey Nguyen",
  "license": "GPL-3.0-or-later",
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "electron-dl": "1.12.0",
    "electron-is-dev": "1.0.1",
    "electron-log": "2.2.17",
    "electron-unhandled": "2.1.0",
    "formik": "1.4.2",
    "prop-types": "15.6.2",
    "puppeteer": "1.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "yup": "0.26.7"
  },
  "devDependencies": {
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "electron": "4.0.1",
    "electron-builder": "20.38.4",
    "electron-devtools-installer": "2.2.4",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "react-scripts": "2.1.3",
    "wait-on": "3.2.0",
    "webpack-bundle-analyzer": "3.0.3"
  },
  "scripts": {
    "start": "concurrently 'yarn react-start' 'wait-on http://localhost:3000/ && yarn electron-start'",
    "react-start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "builder:pack": "electron-builder --dir",
    "builder:dist": "yarn build && electron-builder -lmw",
    "postinstall": "electron-builder install-app-deps",
    "analyze:react": "yarn build --stats && webpack-bundle-analyzer build/bundle-stats.json",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier --write './src/**/*.{ts,tsx,js,jsx,scss,css,json}'"
  },
  "browserslist": [
    "Electron >= 4.0.0"
  ],
  "build": {
    "appId": "com.joeynguyen.tagged-photos-scraper",
    "files": [
      "build",
      "node_modules"
    ],
    "productName": "Tagged Photos Scraper",
    "mac": {
      "category": "public.app-category.utilities"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "yarn lint:write",
      "yarn prettier",
      "git add"
    ]
  }
}
